{
    "name": "boa-lang",
    "displayName": "boa lang",
    "description": "Boa programming language",
    "version": "0.0.1",
    "engines": {
        "vscode": "^1.63.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "contributes": {
        "languages": [{
            "id": "boa",
            "aliases": ["Boa", "boa"],
