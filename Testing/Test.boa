op assertTrue(bool, str) do {
  if bool do {
    print(str + " tested good")
  } else do {
    print(str + " failed")
  }
}

op assertFalse(bool, str) do {
  assertTrue(not bool, str)
}

a = 289
b = 324
c = 6431

assertTrue(b + a == a + b, "commutative_1")
assertTrue(b - a == -(a - b), "commutative_2")
assertTrue(b * a == a * b, "commutative_3")
assertFalse(b / a == a / b, "commutative_4")
assertTrue((a * b) * c == a * (b * c), "associative_1")
assertTrue(a * b + c == 100067, "general_pemdas_1")
assertTrue((a + b) * c == 3942203, "general_pemdas_2")
assertTrue((a * a) + c * a + (b + b) * a == 2129352, "general_pemdas_3")