class Pair(k, v) do {
    op init() do null
}

class Map() do {
    op init() do {
        pairs = []
    }

    op put(key, value) do {
        pairs.append(Pair(key, value))
    }

    op get(key) do {
        for pair : pairs do {
            if pair.k == key do {
                return pair.v
            }
        }
        return null
    }
}

m = Map()
m.put(1, "one")
print(m.get(1))