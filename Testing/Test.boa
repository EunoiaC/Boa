"Values currently stored in the class structure itself"

class Person(name, age) do {
    op init() do {
        x = 12
    }
    op say() do {
        print("Hello, I am " + name + " and I am " + age + " years old")
    }
}

john = Person("John", 23)
alan = Person("Alan", 21)

john.say()
alan.say()