alphabet = "abcdefghijklmnopqrstuvwxyz"
op ceaserify(string, shift) do {
    res = ""

    for char : string do {
        if char == " " do {
            res += " "
            continue
        }
        toGet = (alphabet.indexOf(char) + shift) % 26
        res += alphabet ? toGet
        print(9/2)
    }

    return res
}
print (ceaserify("blue dogs", 1))