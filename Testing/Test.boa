class Bread(type) do {
    op init() do butter = null
    op putButter(butter) do {
        this.butter = butter
    }
    op has(btr) infix do {
        return btr == butter
    }
}

class Butter(type) do {
    op init() do null
}

bread = Bread("Rye")
butter = Butter("Cheddar")

bread.putButter(butter)
print(bread has butter)