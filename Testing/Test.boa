class Pair(k, v) do {
    op init() do null
}

class Map() do {
    op init() do {
        pairs = []
    }

    op put(key, value) do {
        pairs.append(Pair(key, value))
    }

    op get(key) do {
        for pair : pairs do {
            if pair.k == key do {
                return pair.v
            }
        }
        return null
    }

    op toString() do {
        s = "{\n"
        for pair : pairs do {
            s += ("\t" + toStr(pair.k) + ": " + toStr(pair.v) + "\n")
        }
        s += "}"
        return s
    }
}

m = Map()
m.put(1, "one")
m.put(2, "two")
m.put(3, "three")

print(m.get(1))
print(m.get(2))
print(m.get(3))

print(m.toString())