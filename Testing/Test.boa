class Node(data) do {
    op init() do {
        next = null
    }

    op add(n) do {
        if (next == null) do {
            next = n
        } else do {
            next.add(n)
        }
    }

    op _print() do {
        if (next) do {
            next._print()
        }
        print(data)
    }
}

n = Node(1)
n.add(Node(2))
n.add(Node(3))
n.add(Node(4))
n.add(Node(5))

n._print()