f = getFile("words.txt")
words = f.readLines()
alphabetMap = {
    "a": 0,
    "b": 0,
    "c": 0,
    "d": 0,
    "e": 0,
    "f": 0,
    "g": 0,
    "h": 0,
    "i": 0,
    "j": 0,
    "k": 0,
    "l": 0,
    "m": 0,
    "n": 0,
    "o": 0,
    "p": 0,
    "q": 0,
    "r": 0,
    "s": 0,
    "t": 0,
    "u": 0,
    "v": 0,
    "w": 0,
    "x": 0,
    "y": 0,
    "z": 0
}

for word : words do {
    for char : word do {
        alphabetMap[char] += 1
    }
}

values = alphabetMap.values
keys = alphabetMap.keys

print(alphabetMap)

n = 5 # number of top values to print

for i = 0 to n - 1 do {
    max = 0
    maxKey = ""
    for key : keys do {
        if alphabetMap[key] > max do {
            max = alphabetMap[key]
            maxKey = key
        }
    }
    print(maxKey)
    alphabetMap[maxKey] = 0
}